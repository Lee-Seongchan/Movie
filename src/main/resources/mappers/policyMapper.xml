<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cinema.policy.mapper.PolicyMapper">

    <insert id="insertRepo">

        INSERT INTO newsReport SET
        `title`=#{title},
        `content`=#{content},
        `name`=#{name},
        `phoneNumber`=#{phoneNumber},
        `email`=#{email};
    </insert>



</mapper>